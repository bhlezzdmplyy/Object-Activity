// Task 1: Create an object called car with the properties type, model, and color.
let car = {
    type: "Sedan",
    model: "2022",
    color: "Red"
};
console.log("Car object:", car);

// Task 2: Use typeof to check the type of the object and log it to the console.
console.log("Type of car object:", typeof car);

// Task 3: Update the type property to "Toyota" and log the updated object.
car.type = "Toyota";
console.log("Updated car object with new type:", car);

// Task 4: Add a new property wheels with the value 4 and log the updated object.
car.wheels = 4;
console.log("Updated car object with new property wheels:", car);
